{
  "version": 2,
  "builds": [
    {
      "src": "/api/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/main.js"
    },
    {
      "src": "/(.*).(js|css|png|jpg|jpeg|webp|gif|svg|woff2|woff|ttf|otf|eot|ico|json)",
      "dest": "/dist/",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html",
      "continue": true
    }
  ]
}
